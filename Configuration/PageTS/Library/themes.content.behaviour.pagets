themes {
	content {
		# Behaviour for content elements
		behaviour {
			default {
				container = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.container
				container-fluid = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.container_fluid
				fade-in = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.fade_in
				screenreader-only = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.screenreader_only
				screenreader-only-focusable = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.screenreader_only_focusable
				print-hidden = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.print_hidden
				print-visible-block = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.print_visible_block
				print-visible-inline = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.print_visible_inline
				print-visible-inline-block = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.print_visible_inline_block
			}
			table {
				table-responsive = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.table_responsive
			}
			gridelements_pi1 {
				#accordion-first-open = Open first accordion panel
				#navigation-pills = Pills navigation
				#navigation-tabs = Tabs navigation
				
				tabs {
					navigation-pills = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.navigation_pills
					navigation-tabs = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.navigation_tabs
				}
				2ColumnGrid {
					pricing-table = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.pricing_table
				}
				3ColumnGrid {
					pricing-table = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.pricing_table
				}
				4ColumnGrid {
					pricing-table = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.pricing_table
				}
				accordion {
					accordion-first-open = LLL:EXT:theme_bootstrap/Resources/Private/Language/locallang.xlf:content.behaviour.accordion_first_open
				}
				carousel {
					
				}
				tabs {
					
				}
				
			}
		}
	}
}